## html.vm
<html>
<title>
$information.ParserName
</title>
<head>
$information.PackageName
</head>
<body>
<h2>Options</h2>
#foreach($option in $information.getOptionDescriptions())

<h3>$option.Identifier</h3>
<p>$!option.Description<p>
#end
<h2>Option Groups</h2>

#foreach($group in $information.getOptionGroupDescriptions())
<h3>$group.Identifier</h3>
<p>
#foreach($child in $group.getChildren())
  $!child #end
</p>
#end
<h2>Rules</h2>
 
#foreach($rule in $information.getOverrideRuleDescriptions())

<p>
#foreach($assignment in $rule.getAssignments())
Assignment: $assignment.OptionIdentifier = $assignment.Value
#end
</p>
<p>
Condition: $!rule.ConditionText
</p>
#end

#foreach($rule in $information.getValidityRuleDescriptions())

<p>
#foreach($assignment in $rule.getAssignments())
Assignment: $assignment.OptionIdentifier = $assignment.Value
#end
</p>
<p>Condition: $!rule.ConditionText
</p>
#end

#foreach($rule in $information.getFlyRuleDescriptions())

<p>
#foreach($assignment in $rule.getAssignments())
Assignment: $assignment.OptionIdentifier = $assignment.Value
#end
</p>
<p>Condition: $!rule.ConditionText
</p>

#end
</body>
</html>