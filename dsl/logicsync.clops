NAME::
  LogicSync

ARGS::
  Help: {"-h", "-help", "--help"}
  		: "Show the help message."
  
  Files: {}:{file-list}:[between="", mustExist="true", allowMultiple="true"]
  		: "Provide input file(s). Determine their type by their extension."
  Generate: {"-g"}:{file-list}:"Generates files. Files will erase the previous version."
  Merge: {"-m"}:{file-list}:"Merge with existing file. If there is no previous version, creates a new file."

FORMAT::
  Help  | ((Output)* Files (Files | Output)*);  
 
WHERE::
  Main:
    Help | Files;
  Output:
    Generate | Merge;

	